{
  "name": "nexus-js",
  "version": "0.0.1",
  "description": "A TypeScript SDK for Nexus",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "scripts": {
    "build:cjs": "tsc --project tsconfig.cjs.json",
    "build:esm": "tsc --project tsconfig.mjs.json",
    "build": "npm run build:cjs && npm run build:esm"
  },
  "keywords": [
    "nexus",
    "typescript",
    "sdk"
  ],
  "author": "Your Name",
  "license": "MIT",
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "devDependencies": {
    "@types/babel__generator": "^7.6.8",
    "@types/jest": "^29.0.0",
    "@types/winston": "^2.4.4",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "ts-jest": "^29.0.0",
    "typechain": "^8.3.2",
    "typescript": "^4.0.0"
  },
  "dependencies": {
    "@typechain/ethers-v6": "^0.5.1",
    "axios": "^1.7.7",
    "ethers": "^6.13.3",
    "winston": "^3.14.2",
    "zksync-ethers": "^6.13.0"
  }
}